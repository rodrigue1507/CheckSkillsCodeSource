@model IEnumerable<SaveSurveyViewModel>
@{
    ViewData["Title"] = "ShowSaveSurvey";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("SaveSurvey", "Survey", FormMethod.Post))
{
<table>
    <tr>
        <th>Numero</th>
        <th>Nom</th>
        
    </tr>


    @foreach (var survey in Model)
    {
        <tr>
            <td>@survey.surveyName</td>
            <td>@Html.ActionLink("afficher", "ShowSurveyByName", new { type = "button", @class = "btn btn-link", surveyName = survey.surveyName })</td>
        </tr>
    }
</table>
}
